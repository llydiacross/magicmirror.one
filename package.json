{
  "name": "magicmirror",
  "version": "1.1.0",
  "author": "",
  "license": "ISC",
  "_moduleAliases": {
    "@app": "./node_modules/infinitymint/dist/app/",
    "@client": "./node_modules/infinitymint-client/dist/src/"
  },
  "scripts": {
    "test": "mocha",
    "serve": "npm run build && concurrently \"serve -s build\" \"npm run server\"",
    "start": "concurrently \"npm run react\" \"npm run server:dev\" \"npm run api\"",
    "react": "node react/start.js",
    "build": "node react/build.js",
    "console": "npx infinitymint --console",
    "ganache": "npx infinitymint --start-ganache --stay-open --start-express 'false' --show-all-logs",
    "api": "npx infinitymint --start-ganache 'false' --stay-open --start-express 'true' --start-web-socket --show-all-logs",
    "deploy:dev": "npx infinitymint make --start-express 'false' --start-ganache 'false' --project 'magicmirror' --network 'ganache' --recompile --redeploy",
    "deploy": "npx infinitymint make --start-express 'false' --start-ganache 'false' --project 'magicmirror' --network 'polygon' --recompile --redeploy",
    "server": "npx pm2 start server/server.mjs",
    "server:dev": "nodemon server/server.mjs --watch /server/",
    "postinstall": "npx hardhat clean && npx hardhat compile && npx infinitymint compile --project 'magicmirror' --network 'hardhat'"
  },
  "glue": {
    "infinitymint": true
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "quotes": [
        2,
        "double",
        {
          "avoidEscape": true
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      "chrome >= 67",
      "edge >= 79",
      "firefox >= 68",
      "opera >= 54",
      "safari >= 14"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.21.5",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
